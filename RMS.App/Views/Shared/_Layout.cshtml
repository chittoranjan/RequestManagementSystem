<!DOCTYPE html>
<html lang="zxx" class="no-js">
<head>
    <!-- Mobile Specific Meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Favicon-->
    <link rel="shortcut icon" href="~/Content/img/bcc_logo.png">
    <!-- Author Meta -->
    <meta name="author" content="colorlib">
    <!-- Meta Description -->
    <meta name="description" content="">
    <!-- Meta Keyword -->
    <meta name="keywords" content="">
    <!-- meta character set -->
    <meta charset="UTF-8">
    <!-- Site Title -->
    <title>@ViewBag.Title</title>
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,200,400,300,500,600,700" rel="stylesheet">
    <!--
    CSS
    ============================================= -->
    <link rel="stylesheet" href="~/Content/css/linearicons.css">
    <link rel="stylesheet" href="~/Content/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/css/bootstrap.css">
    <link rel="stylesheet" href="~/Content/css/magnific-popup.css">
    <link rel="stylesheet" href="~/Content/css/nice-select.css">
    <link rel="stylesheet" href="~/Content/css/animate.min.css">
    <link rel="stylesheet" href="~/Content/css/jquery-ui.css">
    <link rel="stylesheet" href="~/Content/css/owl.carousel.css">
    <link href="~/Content/chosenDD/chosen.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/css/main.css">
    
    @RenderSection("styles", required: false)
</head>
<body>
    <!--Start Header Menu-->
    @Html.Partial("_MenuBar")
    <!-- End header menu -->

<!-- Start service Area -->
<section class="service-area" style="height: 100vh; min-height: 100vh; overflow: hidden; clear: both; height: auto;">
    <div class="container" style="height: 175px; background-color: #FFF"></div>
    <div class="container">
        <div class="row d-flex justify-content-center">
            <div class="col-md-12 bgnewcolor" style="padding: 20px 20px;">
                <div class="row">
                    <div class="col-md-3">
                        
                        <!--Requisition Que-->
                        @if (User.IsInRole("Controller") || User.IsInRole("Administrator"))
                        {
                            <div class="row" style="font-size: 16px;">

                                <div class="col-md-12">
                                    <h3 class="text-dark">
                                        <i class="fa fa-gear" aria-hidden="true"></i> Queue Item
                                    </h3>
                                    <hr/>
                                </div>
                                <br/><br/>
                                <div class="col-md-12">
                                    <a class="dropdown-item" href="@Url.Action("Requests", "AssignRequisitions")"><i class="fa fa-car"></i> New Requisitions </a>
                                </div>
                                <br/><br/>
                                <div class="col-md-12">
                                    <a class="dropdown-item" href="@Url.Action("Index", "AssignRequisitions")"><i class="fa fa-car"></i> Assigned Requisitions </a>
                                </div>
                                <br/><br/>
                                <div class="col-md-12">
                                    <a class="dropdown-item" href="@Url.Action("Index", "CancelRequisition")"><i class="fa fa-car"></i> Cancelled Requisitions </a>
                                </div>
                                <br/><br/>
                                <div class="col-md-12">
                                    <a class="dropdown-item" href="@Url.Action("OnProcess", "AssignRequisitions")"><i class="fa fa-car"></i> OnProcess Requisitions </a>
                                </div>
                                <br/><br/>
                                <div class="col-md-12">
                                    <a class="dropdown-item" href="@Url.Action("CompleteRequest", "AssignRequisitions")"><i class="fa fa-car"></i> Complete Requisitions </a>
                                </div>

                                <br/><br/>
                                <div class="col-md-12">
                                    <a class="dropdown-item" href="@Url.Action("Index", "RequisitionStatus")"><i class="fa fa-car"></i> Requisitions Status </a>
                                </div>
                                <br/><br/>
                                <div class="col-md-12">
                                    <a class="dropdown-item" href="@Url.Action("Index", "Requisitions")"><i class="fa fa-car"></i> All Requisition</a>
                                </div>
                                <br /><br />
                                <div class="col-md-12">
                                    <a class="dropdown-item" href="@Url.Action("Index", "Schedule")"><i class="fa fa-car"></i> ScheduleCalendar</a>
                                </div>
                                <br />


                            </div>
                        }
                        <!--Mail Service-->
                        @if (User.IsInRole("Controller"))
                        {
                            <br/><br/>
                            <div class="row">
                                <div class="col-md-12">
                                    <h4 class="text-dark">
                                        <i class="fa fa-envelope-open" aria-hidden="true"></i> Mail Service
                                    </h4>
                                    <hr/>
                                </div>
                                <br/><br/>
                                <div class="col-md-12" style="font-size: 16px;">
                                    <div class="col-md-12 col-12">
                                        <a href="#" class="text-black font-weight-normal" data-toggle="collapse" data-target="#demo10"><i class="fa fa-envelope-o" aria-hidden="true"></i> Mail</a><br/>
                                    </div>
                                    <div id="demo10" class="col-md-12 col-12 collapse clearfix" style="margin-left: 35px;">
                                        @Html.ActionLink("Send New", "Create", "MailServices")<br/>
                                        @Html.ActionLink("List Of Mail", "Index", "MailServices")
                                    </div>
                                </div>
                                <br/><br/>
                            </div>
                           

                            <!----Report Menu-->
                            <div class="row">
                                <div class="col-md-12">
                                    <h4 class="text-dark">
                                        <i class="fa fa-envelope-open" aria-hidden="true"></i> Report
                                    </h4>
                                    <hr/>
                                </div>
                                <br/><br/>
                                <div class="col-md-12" style="font-size: 16px;">
                                    <div class="col-md-12 col-12">
                                        <a href="#" class="text-black font-weight-normal" data-toggle="collapse" data-target="#report"><i class="fa fa-envelope-o" aria-hidden="true"></i> Report</a><br/>
                                    </div>
                                    <div id="report" class="col-md-12 col-12 collapse clearfix" style="margin-left: 35px;">
                                        @Html.ActionLink("Assign Requisition", "ReportIndex", "AssignRequisitions")<br/>
                                        
                                    </div>
                                </div>
                                <br/><br/>
                            </div>
                        }
                        @if (User.IsInRole("Controller"))
                        {
                             <!-------Message Menu-->
                            <br /><br />
                            <div class="row">
                                <div class="col-md-12">
                                    <h4 class="text-dark">
                                        <i class="fa fa-envelope-open" aria-hidden="true"></i> Mail Service
                                    </h4>
                                    <hr />
                                </div>
                                <br /><br />
                                <div class="col-md-12" style="font-size: 16px;">
                                    <div class="col-md-12 col-12">
                                        <a href="#" class="text-black font-weight-normal" data-toggle="collapse" data-target="#demo10"><i class="fa fa-envelope-o" aria-hidden="true"></i> Message</a><br />
                                    </div>
                                    <div id="demo10" class="col-md-12 col-12 collapse clearfix" style="margin-left: 35px;">
                                        @Html.ActionLink("Message", "GetMessage", "Queue")<br />
                                    </div>
                                </div>
                                <br /><br />
                            </div>
                        }
                    </div>

                    <div class="col-md-9">
                        @RenderBody()

                        

                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
    <!-- End service Area -->
    
    <!-- start footer Area -->
    <footer class="footer-area ">
        <div class="container">
           
            <div class="row  d-flex justify-content-between">
                <p class="col-lg-8 col-sm-12 footer-text m-0">
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    Copyright &copy;
                    <script>document.write(new Date().getFullYear());</script> All rights reserved | this product make by <i class="fa fa-heart-o" aria-hidden="true"></i> &nbsp; <a href="@Url.Action("AboutDeveloper","Home")" target="_blank"><strong>Developers</strong></a>
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                </p>
                <div class="col-lg-4 col-sm-12 footer-social">
                    <a href="#"><i class="fa fa-facebook"></i></a>
                    <a href="#"><i class="fa fa-twitter"></i></a>
                    <a href="#"><i class="fa fa-google-plus"></i></a>
                    <a href="#"><i class="fa fa-linkedin"></i></a>
                </div>
            </div>
        </div>
    </footer>
<!-- End footer Area -->

    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="~/Content/js/vendor/bootstrap.min.js"></script>
    <script src="~/Content/js/easing.min.js"></script>
    <script src="~/Content/js/hoverIntent.js"></script>
    <script src="~/Content/js/superfish.min.js"></script>
    <script src="~/Content/js/jquery.ajaxchimp.min.js"></script>
    <script src="~/Content/js/jquery.magnific-popup.min.js"></script>
    <script src="~/Content/js/owl.carousel.min.js"></script>
    <script src="~/Content/js/jquery.nice-select.min.js"></script>
    <script src="~/Content/js/jquery-ui.js"></script>
    <script src="~/Content/js/mail-script.js"></script>
    <script src="~/Scripts/chosenDD/chosen.jquery.min.js"></script>
    <script src="~/Content/js/main.js"></script>
    <script src="~/Scripts/RequiredFieldsAsteriskJs/RequiredFieldAsterisk.js"></script>

    @RenderSection("scripts", required: false)
    
</body>
</html>